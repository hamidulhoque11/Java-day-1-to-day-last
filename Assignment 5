//Assignment 5
import java.util.Scanner;
class AssignmentFive {
    public static void main(String[]args){
        Scanner sc=new Scanner(System.in);
        System.out.println("----Check you're a Valid for vote or not-- ");
        System.out.print("Enter your age:");
        int age =sc.nextInt();
        if(age>18){
            System.out.printf("Your age is %d \n\t\t You are valid for vote",age);
        }else if (age<18){
            System.out.printf("Your age is %d \n\t\t You are invalid for vote",age);
        }else{
            System.out.printf("your age is  %d \n\t\t You are valid for vote",age);
        }
    }
}
